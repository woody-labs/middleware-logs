{
    "name": "woody/middleware-logs",
    "description": "Middleware PSR-15 compliant which handle Logs",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastien Malot",
            "email": "sebastien@malot.fr"
        }
    ],
    "require": {
        "php": "^7.1",
        "psr/log": "^1.1",
        "woody/http-server-middleware": "dev-master"
    },
    "suggest": {
        "monolog/monolog": "Add monolog support for advanced log integration"
    },
    "autoload": {
        "psr-4": {
            "Woody\\Middleware\\Logs\\": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
